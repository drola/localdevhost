<h3>{{ path }}</h3>
<table class="table table-striped table-condensed table-hover">
	<thead>
		<th></th>
		<th>Name</th>
		<th>Size</th>
	</thead>
	<tbody>
		{{#each items}}
		<tr>
			<td></td>
			<td><a href="#" data-path="{{ path }}" class="browser-link">{{ name }}</a></td>
			<td>{{fileSize size}}</td>
		</tr>
		{{/each}}
	</tbody>
</table>